<p id="notice"><%= notice %></p>

<h1>Editions</h1>

<table>
  <thead>
    <tr>
      <th>Publication</th>
      <th>Month</th>
      <th>Year</th>
      <th>Number</th>
      <th>Volume</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @editions.each do |edition| %>
      <tr>
        <td><%= edition.publication.name %></td>
        <td><%= edition.month %></td>
        <td><%= edition.year %></td>
        <td><%= edition.number %></td>
        <td><%= edition.volume %></td>
        <td><%= edition.description %></td>
        <td><%= link_to 'Show', edition %></td>
        <td><%= link_to 'Edit', edit_edition_path(edition) %></td>
        <td><%= link_to 'Destroy', edition, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>


<%= link_to 'Home', '/' %>  |
<%= link_to 'New Edition', new_edition_path %>
